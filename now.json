{
  "version": 2,
  "name": "SandBarAndIslandGrill",
  "env": {
    "SENTRY_DSN": "TODO"
  },
  "builds": [
    {
      "src": "graph/package.json",
      "use": "@now/node",
      "config": {
        "includeFiles": ["**/*.graphql"]
      }
    },
    { "src": "www/package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/graphql", "dest": "/graph/index.js" },
    { "src": "/(.*)", "dest": "/www/$1" }
  ]
}
